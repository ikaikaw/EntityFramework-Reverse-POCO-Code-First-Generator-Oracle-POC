	public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionReturnValueFinalStatement = (sp) =>
    {
		var returnParam = sp.Parameters.FirstOrDefault(p => p.Mode == StoredProcedureParameterMode.Return);
