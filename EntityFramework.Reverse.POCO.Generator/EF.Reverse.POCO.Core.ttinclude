    // ** Stored procedure callbacks

    public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionName = sp => sp.NameHumanCase;

    public static readonly Func<StoredProcedure, bool> StoredProcHasOutParams = (sp) =>
    {
        return sp.Parameters.Any(x => x.Mode != StoredProcedureParameterMode.In);
    };

    public static readonly Func<StoredProcedure, bool, string> WriteStoredProcFunctionParams = (sp, includeProcResult) =>
    {
        var sb = new StringBuilder();

		var returnParam = sp.Parameters.FirstOrDefault(x => x.Mode == StoredProcedureParameterMode.Return);
		if (returnParam != null && (returnParam.SqlDbType == "Char" || returnParam.SqlDbType == "NChar")) 
		{
			sb.Append("int returnValueCharLength");
		}

        int n = 1;
		var query = sp.Parameters.Where(x => x.Mode != StoredProcedureParameterMode.Return);
        int count = query.Count();
		if (sb.Length > 0 && count > 0)
		{
			sb.Append(", ");
		}
        foreach (var p in query.OrderBy(x => x.Ordinal))
        {
            sb.AppendFormat("{0}{1}{2} {3}{4}",
                p.Mode == StoredProcedureParameterMode.Out ? "out " : p.Mode == StoredProcedureParameterMode.InOut ? "ref " : "",
                p.PropertyType,
                NotNullable.Contains(p.PropertyType.ToLower()) ? string.Empty : "?",
                p.NameHumanCase,
                (n++ < count) ? ", " : string.Empty);

			if (p.Mode != StoredProcedureParameterMode.In && (p.SqlDbType == "Char" || p.SqlDbType == "NChar")) 
			{
				count++;
				sb.AppendFormat("int {0}{1}",
					$"{p.NameHumanCase}CharLength",
					(n++ < count) ? ", " : string.Empty);
			}
        }
        return sb.ToString();
    };

	public static readonly Func<StoredProcedure, string> WriteStoredProcOutParametersInit = (sp) =>
    {
		var sb = new StringBuilder();
        foreach (var p in sp.Parameters.Where(x => x.Mode == StoredProcedureParameterMode.Out).OrderBy(x => x.Ordinal))
        {
			sb.AppendFormat("            {0} = null;", p.NameHumanCase).AppendLine();
		}
		if (sb.Length > 0) 
		{
			sb.Insert(0, "\n");
			return sb.ToString();
		}
		return string.Empty;
    };

    public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionOverloadCall = (sp) =>
    {
        var sb = new StringBuilder();
        foreach (var p in sp.Parameters.OrderBy(x => x.Ordinal))
        {
            sb.AppendFormat("{0}{1}, ",
                p.Mode == StoredProcedureParameterMode.In ? "" : "out ",
                p.NameHumanCase);
        }
        sb.Append("out procResult");
        return sb.ToString();
    };

    public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionSqlAtParams = sp =>
    {
		var query = sp.Parameters.Where(p => p.Mode != StoredProcedureParameterMode.Return);
		int count = query.Count();
			
		if(count > 0) {
			var sb = new StringBuilder();
			int n = 1;
			sb.Append("(");
			foreach (var p in query.OrderBy(x => x.Ordinal))
			{
				sb.AppendFormat(":{0}{1}",
					p.Name,
					(n++ < count) ? ", " : string.Empty);
			}
			sb.Append(")");
			return sb.ToString();
		}
		return string.Empty;
    };

    public static readonly Func<StoredProcedureParameter, string> WriteStoredProcSqlParameterName = p => p.Mode == StoredProcedureParameterMode.Return ? "returnValueParam" : $"{p.NameHumanCase}Param";

	public static readonly Func<StoredProcedureParameter, string> GetStringOutParamSize = (p) => 
	{
		if (p.SqlDbType == "Clob" || p.SqlDbType == "NClob")
		{
			return string.Empty;
		}
		if (p.SqlDbType == "Char" || p.SqlDbType == "NChar")
		{
			return p.Mode == StoredProcedureParameterMode.Return ? ", Size = returnValueCharLength" : $", Size = {p.NameHumanCase}CharLength";
		}
		return ", Size = 32767";
	};

    public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionDeclareSqlParameter = (sp) =>
    {
        var sb = new StringBuilder();
        foreach (var p in sp.Parameters.OrderBy(x => x.Ordinal))
        {
            var isNullable = !NotNullable.Contains(p.PropertyType.ToLower());
            var getValueOrDefault = isNullable ? ".GetValueOrDefault()" : string.Empty;
            var isGeography = p.PropertyType == "System.Data.Entity.Spatial.DbGeography";

            sb.AppendLine(
                $"            var {WriteStoredProcSqlParameterName(p)} = new Oracle.ManagedDataAccess.Client.OracleParameter"
                + (p.Mode == StoredProcedureParameterMode.Return ? " { ParameterName = \":returnValue\", " : $" {{ ParameterName = \":{p.Name}\", ")
                + $"OracleDbType = OracleDbType.{p.SqlDbType}"
                + ", Direction = System.Data.ParameterDirection."
                + (p.Mode == StoredProcedureParameterMode.In ? "Input" : p.Mode == StoredProcedureParameterMode.Out ? "Output": p.Mode == StoredProcedureParameterMode.InOut ? "InputOutput" : p.Mode == StoredProcedureParameterMode.Return ? "ReturnValue" : "Input")
                + (p.Mode == StoredProcedureParameterMode.In || p.Mode == StoredProcedureParameterMode.InOut ? ", Value = " + p.NameHumanCase + getValueOrDefault : string.Empty)
				+ (p.OdpNetType == "OracleString" && p.PropertyType == "string" ? p.Mode == StoredProcedureParameterMode.In ? ", Size = " + p.NameHumanCase + ".Length" : GetStringOutParamSize(p) : string.Empty)
				+ " };");

                if (p.Mode == StoredProcedureParameterMode.In)
                {
                    sb.AppendFormat(
                        isNullable
                            ? "            if (!{0}.HasValue){1}                {0}Param.Value = System.DBNull.Value;{1}{1}"
                            : "            if ({0}Param.Value == null){1}                {0}Param.Value = System.DBNull.Value;{1}{1}",
                        p.NameHumanCase, Environment.NewLine);
                }
        }
		if (sb.Length > 0)
		{
			sb.Insert(0, "\n");
			return sb.ToString();
		}
		return string.Empty;
    };

	public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionReturnValueAssignment = sp =>
    {
		var hasReturnParam = sp.Parameters.Any(p => p.Mode == StoredProcedureParameterMode.Return);
		if (hasReturnParam)
		{
			return ":returnValue := ";
		}
		return string.Empty;
    };

    public static readonly Func<StoredProcedure, string> WriteTableValuedFunctionDeclareSqlParameter = sp =>
    {
        var sb = new StringBuilder();
        foreach (var p in sp.Parameters.OrderBy(x => x.Ordinal))
        {
            sb.AppendLine(string.Format("            var {0}Param = new System.Data.Entity.Core.Objects.ObjectParameter(\"{1}\", typeof({2})) {{ Value = (object){3} }};",
                p.NameHumanCase,
                p.Name.Substring(1),
                p.PropertyType,
                p.NameHumanCase + (p.Mode == StoredProcedureParameterMode.In && NotNullable.Contains(p.PropertyType.ToLowerInvariant()) ? string.Empty : " ?? System.DBNull.Value" )));
        }
        return sb.ToString();
    };

    public static readonly Func<StoredProcedure, string> WriteStoredProcFunctionSqlParameterAnonymousArray = (sp) =>
    {
        var sb = new StringBuilder();
        bool hasParam = false;
        foreach (var p in sp.Parameters.OrderBy(x => x.Ordinal))
        {
			if(p.Mode == StoredProcedureParameterMode.Return)
				sb.Append(string.Format("returnValueParam, ", p.NameHumanCase));
			else
				sb.Append(string.Format("{0}Param, ", p.NameHumanCase));
            hasParam = true;
        }
        if(hasParam) {
			sb.Insert(0, "cmd.Parameters.AddRange(new[] {");
            sb.Remove(sb.Length - 2, 2);
			sb.Append("});");
		}
        return sb.ToString();
    };

    public static readonly Func<StoredProcedure, string> WriteTableValuedFunctionSqlParameterAnonymousArray = sp =>
    {
        if (sp.Parameters.Count == 0)
            return "new System.Data.Entity.Core.Objects.ObjectParameter[] { }";
        var sb = new StringBuilder();
        foreach (var p in sp.Parameters.OrderBy(x => x.Ordinal))
        {
            sb.Append(string.Format("{0}Param, ", p.NameHumanCase));
        }
        return sb.ToString().Substring(0, sb.Length - 2);
    };

    public static readonly Func<StoredProcedure, bool, string> WriteStoredProcFunctionSetSqlParameters = (sp, isFake) =>
    {
        var sb = new StringBuilder();
        foreach (var p in sp.Parameters.Where(x => x.Mode != StoredProcedureParameterMode.In).OrderBy(x => x.Ordinal))
        {
            var Default = string.Format("default({0})", p.PropertyType);
            var notNullable = NotNullable.Contains(p.PropertyType.ToLower());

			var paramVarName = p.Mode == StoredProcedureParameterMode.Return ? "returnValueParam" : string.Format("{0}Param", p.NameHumanCase);
			var varName = p.Mode == StoredProcedureParameterMode.Return ? "returnValue" : p.NameHumanCase;
            if(isFake)
                sb.AppendLine(string.Format("                    {0} = {1};", varName, Default));
            else
            {
				sb.AppendLine("");
                sb.AppendLine(string.Format("                    if (IsSqlParameterNull({0}))", paramVarName));
                sb.AppendLine(string.Format("                        {0} = {1};", varName, notNullable ? Default : "null"));
                sb.AppendLine("                    else");
                sb.AppendLine(string.Format("                        {0} = ({1}) ((Oracle.ManagedDataAccess.Types.{2}) {3}.Value).Value;", varName, p.PropertyType, p.OdpNetType, paramVarName));
            }
        }
		if (sb.Length > 0)
		{
			sb.Insert(0, "\n");
			return sb.ToString();
		}
        return string.Empty;
    };

    public static readonly Func<StoredProcedure, string> WriteStoredProcReturnModelName = sp =>
    {
        if (Settings.StoredProcedureReturnTypes.ContainsKey(sp.NameHumanCase))
            return Settings.StoredProcedureReturnTypes[sp.NameHumanCase];
        if (Settings.StoredProcedureReturnTypes.ContainsKey(sp.Name))
            return Settings.StoredProcedureReturnTypes[sp.Name];

        var name = string.Format("{0}ReturnModel", sp.NameHumanCase);
        if (Settings.StoredProcedureReturnModelRename != null)
        {
            var customName = Settings.StoredProcedureReturnModelRename(name, sp);
            if (!string.IsNullOrEmpty(customName))
                name = customName;
        }

        return name;
    };

    public static readonly Func<StoredProcedure, string> WriteStoredProcReturnType = (sp) =>
    {
		var returnParam = sp.Parameters.FirstOrDefault(p => p.Mode == StoredProcedureParameterMode.Return);
		if (returnParam == null)
		{
			return "void";
		}
        return string.Format("{0}{1}", returnParam.PropertyType, NotNullable.Contains(returnParam.PropertyType.ToLower()) ? string.Empty : "?");
